<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <style name="Widget.Topface.ActionBar" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="backgroundStacked">@drawable/bg_actionbar</item>
        <item name="backgroundSplit">@drawable/bg_actionbar</item>
        <item name="titleTextStyle">@style/Widget.Topface.ActionBar.Title</item>
        <item name="subtitleTextStyle">@style/Widget.Topface.ActionBar.Subtitle</item>
        <item name="contentInsetStart">0dp</item>
        <item name="background">@drawable/bg_actionbar</item>
    </style>

    <style name="Widget.Topface.TransparentActionBar" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="contentInsetStart">0dp</item>
        <item name="background">@android:color/transparent</item>
        <item name="icon">@android:color/transparent</item>
    </style>

    <style name="Widget.Topface.ActionBar.Title" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:textSize">@dimen/action_bar_title_size</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Widget.Topface.ActionBar.Subtitle" parent="TextAppearance.AppCompat.Widget.ActionBar.Subtitle">
        <item name="android:textColor">#99FFFFFF</item>
        <item name="android:textSize">@dimen/action_bar_subtitle_size</item>
    </style>

    <style name="topfaceEditText">
        <item name="android:textCursorDrawable" tools:ignore="NewApi">@null</item>
        <item name="android:cursorVisible">true</item>
        <item name="android:background">@drawable/edittext_selector</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">#393939</item>
        <item name="android:focusable">true</item>
