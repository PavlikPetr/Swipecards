<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Profile -->
    <PreferenceCategory android:title="@string/profile_header_title" >
        
        <!-- Name -->
        <Preference
            android:key="@string/s_profile_name"
            android:title="@string/profile_form_name"
            android:summary="name" />
        
        <!-- sex -->
        <Preference
            android:key="@string/s_profile_sex"
            android:title="@string/filter_sex" 
            android:summary="sex" />
        
        <!-- age -->
        <Preference
            android:key="@string/s_profile_age"
            android:title="@string/filter_age" 
            android:summary="age" />
        
        <!-- city -->
        <Preference
            android:key="@string/s_profile_city"
            android:title="@string/filter_city" 
            android:summary="city" />
        
      
    </PreferenceCategory>

    <!-- Profile -->
    <PreferenceCategory android:title="@string/profile_btn_form" >
        
        <!-- about -->
        <Preference
            android:key="@string/s_profile_about"
            android:title="@string/profile_status" 
            android:summary="This allows you to enter a string" />
        
        <!-- height -->
        <Preference
            android:key="@string/s_profile_height"
            android:title="@string/profile_height" 
            android:summary="This allows you to enter a string" />
        
        <!-- weight -->
        <Preference
            android:key="@string/s_profile_weight"
            android:title="@string/profile_weight" 
            android:summary="This allows you to enter a string" />
        
        <!-- fitness -->
        <ListPreference
            android:key="@string/s_profile_fitness"
            android:title="@string/profile_fitness" 
            android:summary="This allows you to enter a string" />
        
        <!-- marriage -->
        <ListPreference
            android:key="@string/s_profile_marriage"
            android:title="@string/profile_marriage" 
            android:summary="This allows you to enter a string" />
        
        <!-- education -->
        <ListPreference
            android:key="@string/s_profile_education"
            android:title="@string/profile_education" 
            android:summary="This allows you to enter a string" />
        
        <!-- finances -->
        <ListPreference
            android:key="@string/s_profile_finances"
            android:title="@string/profile_finances" 
            android:summary="This allows you to enter a string" />
        
        <!-- smoking -->
        <ListPreference
            android:key="@string/s_profile_smoking"
            android:title="@string/profile_smoking" 
            android:summary="This allows you to enter a string" />
        
        <!-- alcohol -->
        <ListPreference
            android:key="@string/s_profile_alcohol"
            android:title="@string/profile_alcohol" 
            android:summary="This allows you to enter a string" />
        
        <!-- commutability -->
        <ListPreference
            android:key="@string/s_profile_commutability"
            android:title="@string/profile_commutability" 
            android:summary="This allows you to enter a string" />
        
        <!-- character -->
        <ListPreference
            android:key="@string/s_profile_character"
            android:title="@string/profile_character" 
            android:summary="This allows you to enter a string" />
      
    </PreferenceCategory>
    
</PreferenceScreen>